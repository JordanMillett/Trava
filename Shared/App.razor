<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(Shared.Layout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(Shared.Layout)">
            <PageTitle>404 Not Found</PageTitle>
            <p>404 - Page Not Found</p>
        </LayoutView>
    </NotFound>
</Router>
